        <!--
    REFERENCE
      * Members labeled with ? are not mandatory.
      * StatIds can be found in external reference.
      * Sizes and offsets of elements are defined in pixel sizes to keep the proportions.

    ............................................
    ALIGN ENUM
        HORISONTAL_LEFT_AND_VERTICAL_TOP = 0,
        HORISONTAL_LEFT_AND_VERTICAL_CENTER = 1,
        HORISONTAL_LEFT_AND_VERTICAL_BOTTOM = 2,
        HORISONTAL_CENTER_AND_VERTICAL_TOP = 3,
        HORISONTAL_CENTER_AND_VERTICAL_CENTER = 4,
        HORISONTAL_CENTER_AND_VERTICAL_BOTTOM = 5,
        HORISONTAL_RIGHT_AND_VERTICAL_TOP = 6,
        HORISONTAL_RIGHT_AND_VERTICAL_CENTER = 7,
        HORISONTAL_RIGHT_AND_VERTICAL_BOTTOM = 8,
    ............................................
    VisibleCondition
        Below, 
        Above, 
        Equal, 
        NotEqual
    ............................................
    Blink TEMPLATE
      <Blink>
        <MinAlpha></MinAlpha> ?
        <MaxAlpha></MaxAlpha> ?
        <IntervalMs></IntervalMs> ?
        <ColorMask> ?
          <X></X>
          <Y></Y>
          <Z></Z>
          <W></W>
        </ColorMask>
        <Blink>true/false</Blink> ?
      </Blink>
    ............................................
    STAT VISUAL STYLE TEMPLATE
      * Is collection of modifiers that are common for all types of 
        Stat styles that are defined below.
      * Right now we are supporting 4 types of stat styles.
        * MyObjectBuilder_CircularProgressBarStatVisualStyle
        * MyObjectBuilder_ProgressBarStatVisualStyle
        * MyObjectBuilder_TextStatVisualStyle
        * MyObjectBuilder_ImageStatVisualStyle
      * This is just a base and should never be used alone without
        specific style definition


    <StatStyle xsi:type="SPECIFIC_OBJECT_BUILDER">
        <StatId></StatId>
        <StatCriticalValue></StatCriticalValue> ?
        <VisibleCondition></VisibleCondition> ?
        <ConditionValue></ConditionValue> ?

        <OffsetPx>
          <X></X>
          <Y></Y>
        </OffsetPx>
        <SizePx>
          <X></X>
          <Y></Y>
        </SizePx>

        <Blink>... BLINK TEMPLATE ...</Blink>

        ...
        STYLE SPECIFIC STUFF
        ...
    </StatStyle>


                ............................................
                MyObjectBuilder_ImageStatVisualStyle
                
                <StatStyle xsi:type="MyObjectBuilder_ImageStatVisualStyle">
                  ...
                  BASE SPECIFIC DEFINITION
                  ...

                  <Texture></Texture>
                  <ColorMask>
                    <X></X>
                    <Y></Y>
                    <Z></Z>
                    <W></W>
                  </ColorMask>
                </StatStyle>
                ............................................
                MyObjectBuilder_TextStatVisualStyle
                
                  * In the text field use {STAT} as a identifier where you want the stat
                    numeric value be displayed. 
                    (example: <Text>Your current speed is: {STAT} m/s</Text>)
                  * Vanilla fonts:
                      * Debug
                      * Red
                      * Green
                      * Blue
                      * White
                      * DarkBlue

                <StatStyle xsi:type="MyObjectBuilder_TextStatVisualStyle">
                  ...
                  BASE SPECIFIC DEFINITION
                  ...

                  <Text></Text>
                  <Scale></Scale>
                  <Font></Font>
                  <TextAlign></TextAlign> ?
                  <ColorMask> ?
                    <X></X>
                    <Y></Y>
                    <Z></Z>
                    <W></W>
                  </ColorMask>
                </StatStyle>
                ............................................
                MyObjectBuilder_ProgressBarStatVisualStyle
                  * Texture field refers to 9Tiled texture definition (Composite texture)
                  * Probably subject to change ... depends ...

                <StatStyle xsi:type="MyObjectBuilder_ProgressBarStatVisualStyle">
                  ...
                  BASE SPECIFIC DEFINITION
                  ...

                  <Texture></Texture>
                  <ColorMask> ?
                    <X></X>
                    <Y></Y>
                    <Z></Z>
                    <W></W>
                  </ColorMask>
                </StatStyle>
                ............................................
                MyObjectBuilder_CircularProgressBarStatVisualStyle
                  * Optional: segment origin
                    * Texture mid is takne when not specified
                  * Optional: Spacing angle
                    * Angle between segments
                  * Optional: Angle offset
                    * Segmentation starts at (-1,0)
                  * Optional: Number of segments
                    * Default value is 10

                <StatStyle xsi:type="MyObjectBuilder_CircularProgressBarStatVisualStyle">
                  ...
                  BASE SPECIFIC DEFINITION
                  ...

                  <SegmentSizePx>
                    <X></X>
                    <Y></Y>
                  </SegmentSizePx>
                  <Segmenttexture></Segmenttexture>
                  <BackgroundTexture></BackgroundTexture> ?

                  <SegmentOrigin> ?
                    <X></X>
                    <Y></Y>
                  </SegmentOrigin>
                  <SpacingAngle></SpacingAngle> ?
                  <AngleOffset></AngleOffset> ?
                  <NumberOfSegments></NumberOfSegments> ?
                  <ShowEmptySegments>true/false</ShowEmptySegments> ?

                  <EmptySegmentColorMask> ?
                    <X></X>
                    <Y></Y>
                    <Z></Z>
                    <W></W>
                  </EmptySegmentColorMask>
                  <FullSegmentColorMask> ?
                    <X></X>
                    <Y></Y>
                    <Z></Z>
                    <W></W>
                  </FullSegmentColorMask>
                  <AnimatedSegmentColorMask> ?
                    <X></X>
                    <Y></Y>
                    <Z></Z>
                    <W></W>
                  </AnimatedSegmentColorMask>

                  <Animate>true/false</Animate> ?
                  <AnimationDelayMs></AnimationDelayMs> ?
                  <AnimationSegmentDelayMs></AnimationSegmentDelayMs> ?
                </StatStyle>
    ............................................
    STAT CONTROL TEMPLATE
      <StatControl>
        <Position>
          <X></X>
          <Y></Y>
        </Position>
        <OriginAlign></OriginAlign>

        <StatStyles>
          <StatStyle>... STAT VISUAL TEMPLATE ...</StatStyle>
        </StatStyles>
      </StatControl>
    ............................................
    -->
