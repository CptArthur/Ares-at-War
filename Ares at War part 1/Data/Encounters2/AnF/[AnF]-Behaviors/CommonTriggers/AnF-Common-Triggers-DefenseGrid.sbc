<!-- RedAlert Mechanics -->
     	
	
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AnF_DefenseSystem_Trigger_RedAlert</SubtypeId>
      </Id>
      <Description>

	[RivalAI Trigger]
	[Type:Timer]
	[UseTrigger:true]
	[StartsReady:true]		


	[MinCooldownMs:9000]
	[MaxCooldownMs:9001]
	[Conditions:FAC-Context-Condition-SettingBattle]
	[MaxActions:-1]
	[Actions:AnF-DefenseSystem-Action-RedAlert]
	
      </Description>
      
    </EntityComponent>  

  
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AnF_DefenseSystem_Action_RedAlert</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]
		
	[BroadcastCommandProfiles:true]
	[CommandProfileIds:AnF-DefenseSystem-Command-RedAlert]


      </Description>
      
    </EntityComponent>    
  

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AnF_DefenseSystem_Command_RedAlert</SubtypeId>
      </Id>
      <Description>

      [RivalAI Command]
      
	[CommandCode:AnF-RedAlert]
	[MatchSenderReceiverOwners:true]
	[SendTargetEntityId:true]

	[CommandDelayTicks:180]

      </Description>
      
    </EntityComponent>   
	
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AnF_DefenseSystem_Trigger_ReceivedRedAlert</SubtypeId>
      </Id>
      <Description>

	[RivalAI Trigger]
		[UseTrigger:true]
	[Type:CommandReceived]
	[StartsReady:true]		
	
	[MinCooldownMs:90000]
	[MaxCooldownMs:120000]
	
	[CommandReceiveCode:AnF-RedAlert]

	[MaxActions:-1]
	[ActionExecution:All]

	[Actions:AnF-DefenseSystem-Action-ReceivedRedAlert]
	
      </Description>
      
    </EntityComponent> 

  	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AnF_DefenseSystem_Action_ReceivedRedAlert</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]
	[InheritLastAttackerFromCommand:true]	

  
      </Description>
      
    </EntityComponent> 

	
