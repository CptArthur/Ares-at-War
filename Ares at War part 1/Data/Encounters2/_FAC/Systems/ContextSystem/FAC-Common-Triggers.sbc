<!-- Actions --> 



	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Action_Hostile_PlayerDamage</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

	[UseChatBroadcast:true]
	[ChatData:FAC-Common-Chat-Hostile-PlayerDamage]
	[IncreaseCounters:CountPlayerDamage]


      </Description>
      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Action_Neutral_PlayerDamage</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

	[UseChatBroadcast:true]
	[ChatData:FAC-Common-Chat-Neutral-PlayerDamage]
	
	[IncreaseCounters:CountPlayerDamage]	
	
	[ChangeReputationWithPlayers:true]
	[ReputationChangeRadius:2000]
	[ReputationChangeFactions:{Self}]
	[ReputationChangeAmount:-50]
	[ReputationChangesForAllRadiusPlayerFactionMembers:true]
	  
      </Description>
      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Action_Friendly_PlayerDamage</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

	[UseChatBroadcast:true]
	[ChatData:FAC-Common-Chat-Friendly-PlayerDamage]
	
	[ChangeReputationWithPlayers:true]
	[ReputationChangeRadius:2000]
	[ReputationChangeFactions:{Self}]
	[ReputationChangeAmount:-10]
	[ReputationChangesForAllRadiusPlayerFactionMembers:true]

      </Description>
      
    </EntityComponent>


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Action_PlayerHelpedNPC</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]
	[EnableTriggers:true]
	[EnableTriggerNames:FAC-Common-Trigger-Compromised-PlayerHelpedNPC]

	[DisableTriggers:true]
	[DisableTriggerNames:FAC-Common-Trigger-Compromised-PlayerNotHelpedNPC]

	


      </Description>
      
    </EntityComponent>



	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Action_Compromised_PlayerHelpedNPC</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

	[UseChatBroadcast:true]
	[ChatData:FAC-Common-Chat-PlayerHelpedNPC]
	[BroadcastCommandProfiles:true]
	[CommandProfileIds:FAC-Common-Command-PlayerHelpedNPC]
	


      </Description>
      
    </EntityComponent>


    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Command_PlayerHelpedNPC</SubtypeId>
      </Id>
      <Description>

	[RivalAI Command]

	[CommandCode:{FactionTag}DestoyedwithPlayerHelp]
	[MatchSenderReceiverOwners:false]
	[SingleRecipient:true]
	[IgnoreAntennaRequirement:true]
	[IgnoreReceiverAntennaRequirement:true]
	[Radius:8000]
	
	</Description>
      
    </EntityComponent>



	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Action_Compromised_PlayerNotHelpedNPC</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

	[UseChatBroadcast:true]
	[ChatData:FAC-Common-Chat-PlayerNotHelpedNPC]
	

      </Description>
      
    </EntityComponent>







	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Chat_Hostile_PlayerDamage</SubtypeId>
      </Id>
	  
      <Description>

      [RivalAI Chat]
      
      [UseChat:false]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:true]
      
      [Author:Debug {AntennaName}]
      [Color:Red]
      
      [ChatMessages:Hostile PlayerDamage Detected]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      

      </Description>
      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Common_Chat_Neutral_PlayerDamage</SubtypeId>
      </Id>
	  
      <Description>

	[RivalAI Chat]

	[UseChat:true]
	[StartsReady:true]
	[Chance:100]
	[MaxChats:-1]
	[BroadcastRandomly:true]

	[Author:{AntennaName}]
	[Color:White]
	[MinTime:2]
	[MaxTime:3]   
	
      [ChatMessages:Have you lost your mind?]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      
      [ChatMessages:What are you doing?!]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      
      [ChatMessages:Save it for the enemy!]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]

      [ChatMessages:Ow! Was it something I said?]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]  
      

      </Description>
      
    </EntityComponent>


