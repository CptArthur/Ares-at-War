      [Author:{AntennaName}]
      [Color:Green]
      
      [ChatMessages:Have you lost your mind?]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      
      [ChatMessages:What are you doing?!]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      
      [ChatMessages:Save it for the enemy!]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]

      [ChatMessages:Ow! Was it something I said?]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]     
      

      </Description>
      
