<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>


	<!-- ================== -->
	<!--    Compromised    -->
	<!-- ================== -->	


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Trigger_HasTarget</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
      [Type:HasTarget]
	  [UseTrigger:true]
	  [StartsReady:true]

	  [MaxActions:-1]
	  [Actions:FAC-Patrol-Action-HasTarget]
	  
      </Description>
      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Trigger_NoTarget</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
      [Type:NoTarget]
	  [UseTrigger:false]
	  [StartsReady:true]
	  [MaxActions:-1]
	  [Actions:FAC-Patrol-Action-NoTarget]
	  
      </Description>
      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Condition_NoTarget</SubtypeId>
      </Id>
      <Description>
      [RivalAI Condition]
      
	[UseConditions:true]
	[MatchAnyCondition:true]
	[CheckTrueBooleans:true]
	[TrueBooleans:PatrolHasnoTarget]
	  
	  
	  
      </Description>
      
    </EntityComponent>


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Trigger_SwitchtoPatrol</SubtypeId>
      </Id>
      <Description>

	[RivalAI Trigger]
	[Type:Timer]
	[UseTrigger:false]
	[StartsReady:false]
	[Conditions:FAC-Patrol-Condition-NoTarget]
	  
	[MinCooldownMs:6000]
	[MaxCooldownMs:9000]
	  
	[Actions:FAC-Patrol-Action-SwitchtoPatrol]
	  
      </Description>
      
    </EntityComponent>



	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Action_HasTarget</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]
	[ChangeBehaviorSubclass:true]  
	[NewBehaviorSubclass:Fighter]

	[UseChatBroadcast:true]
	[ChatData:FAC-Patrol-Chat-HasTarget]
	[ChangeAutopilotProfile:true]
	[AutopilotProfile:Secondary]	

	[ChangeAutopilotSpeed:true]
	[NewAutopilotSpeed:-1]

	
	
	[EnableTriggers:true]
	[EnableTriggerNames:FAC-Patrol-Trigger-NoTarget]
	
	[DisableTriggers:true]
	[DisableTriggerNames:FAC-Patrol-Trigger-HasTarget]	
	[DisableTriggerNames:FAC-Patrol-Trigger-SwitchtoPatrol]		
	
	
	[ResetCooldownTimeOfTriggers:true]
	[ResetTriggerCooldownNames:FAC-Patrol-Trigger-SwitchtoPatrol]	
	
	
	[SetBooleansFalse:PatrolHasnoTarget]	


      </Description>
      
    </EntityComponent>		


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Action_NoTarget</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]
	[SetBooleansTrue:PatrolHasnoTarget]

	[DisableTriggers:true]
	[DisableTriggerNames:FAC-Patrol-Trigger-NoTarget]

	[ResetCooldownTimeOfTriggers:true]
	[ResetTriggerCooldownNames:FAC-Patrol-Trigger-SwitchtoPatrol]	
	
	
	[EnableTriggers:true]
	[EnableTriggerNames:FAC-Patrol-Trigger-HasTarget]
	[EnableTriggerNames:FAC-Patrol-Trigger-SwitchtoPatrol]
		
		
	[DisableTriggers:true]
	[DisableTriggerNames:FAC-Patrol-Trigger-NoTarget]		

      </Description>
      
    </EntityComponent>		

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Action_SwitchtoPatrol</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

	[DisableTriggers:true]
	[DisableTriggerNames:FAC-Patrol-Trigger-SwitchtoPatrol]

	[ChangeBehaviorSubclass:true]  
	[NewBehaviorSubclass:Patrol]

	[UseChatBroadcast:true]
	[ChatData:FAC-Patrol-Chat-SwitchtoPatrol]


		
      </Description>
      
    </EntityComponent>


	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Chat_HasTarget</SubtypeId>
      </Id>
	  
      <Description>

      [RivalAI Chat]
      
      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:true]
      
      [Author:{AntennaName}]
      [Color:Red]
      
      [ChatMessages:I have a target, switching to fighter]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      

      </Description>
      
    </EntityComponent>
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>FAC_Patrol_Chat_SwitchtoPatrol</SubtypeId>
      </Id>
	  
      <Description>

      [RivalAI Chat]
      
      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:true]
      
      [Author:{AntennaName}]
      [Color:Red]
      
      [ChatMessages:No Targets, switching to Patrol]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
      

      </Description>
      
    </EntityComponent>	

	
	
  </EntityComponents>
</Definitions>