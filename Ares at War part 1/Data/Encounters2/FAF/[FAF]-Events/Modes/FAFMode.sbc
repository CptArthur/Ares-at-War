<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>


<!--
FAFMode



0: OntheRun
1: Secret
2: OpenWar
3: AllColonies
4: FreeAgarisRepublic
Bool FAFDestroyed


[CheckCustomCounters:true]
[CustomCounters:FAFMode]
[CustomCountersTargets:2]
[CounterCompareTypes:LessOrEqual]

	

AaW2-Event-SetFAFOntheRun
AaW2-Event-SetFAFSecret
AaW2-Event-SetFAFOpenWar

AaW2-Event-SetFAFDestroyed

-->



    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW2_Event_SetFAFOntheRun</SubtypeId>
      </Id>
      <Description>

		[MES Event]
		[UniqueEvent:false]
		[Tags:FAF]		
		[ConditionIds:AaW-EventCondition-SetFAFOntheRun]
		[ActionExecution:All]

		[ActionIds:AaW-EventAction-SetFAFOntheRun]

      </Description>
	</EntityComponent>	
      
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_EventCondition_SetFAFOntheRun</SubtypeId>
      </Id>
      <Description>

      [MES Event Condition]
	  
	[CheckCustomCounters:true]
	[CustomCounters:FAFMode]
	[CustomCountersTargets:0]
	[CounterCompareTypes:NotEqual]
	
	  [CheckTrueBooleans:true]
	  [TrueBooleans:FAF-HQDestroyed]
	  
	  
      [CheckFalseBooleans:true]
      [FalseBooleans:FAFSunsetCity]
      [FalseBooleans:FAFCarcosa]
      [FalseBooleans:FAFAzuris]	  

  
	  
      </Description>
      
    </EntityComponent>	

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_EventAction_SetFAFOntheRun</SubtypeId>
      </Id>
      <Description>

		[MES Event Action]
		[ChangeCounters:true]
		[SetCounters:FAFMode]
		[SetCountersAmount:0]	
		
		




      </Description>
      
    </EntityComponent>	
	
	
	


    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW2_Event_SetFAFSecret</SubtypeId>
      </Id>
      <Description>

		[MES Event]
		[UniqueEvent:false]
