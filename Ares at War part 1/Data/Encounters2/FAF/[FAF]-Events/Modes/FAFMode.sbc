      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_EventAction_SetFAFOpenWar</SubtypeId>
      </Id>
      <Description>

		[MES Event Action]
		[ChangeCounters:true]
		[SetCounters:FAFMode]
		[SetCountersAmount:2]		

      </Description>
      
    </EntityComponent>	
	
	
