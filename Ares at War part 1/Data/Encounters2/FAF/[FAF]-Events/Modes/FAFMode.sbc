    </EntityComponent>	

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_EventAction_SetFAFSecret</SubtypeId>
      </Id>
      <Description>

		[MES Event Action]
		
		
		[ChangeCounters:true]
		[SetCounters:FAFMode]
		[SetCountersAmount:1]		

      </Description>
      
    </EntityComponent>	



    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW2_Event_SetFAFOpenWar</SubtypeId>
