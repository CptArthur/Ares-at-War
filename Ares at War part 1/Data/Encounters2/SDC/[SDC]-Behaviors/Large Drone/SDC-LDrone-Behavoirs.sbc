      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>CRUSADERS_LDroneTaskLeader_Behavior</SubtypeId>
      </Id>
      <Description>

	[RivalAI Behavior]
	[BehaviorName:Passive]
	[AutopilotData:CRUSADERS-LDroneAutopilot]	
	[SecondaryAutopilotData:CRUSADERS_LDroneAutopilot_Horsefly]	
	[TargetData:AaW_Target_Main_A]		
	
	
	[TriggerGroups:ContextTriggers]	 
	[TriggerGroups:CRUSADERS_CommonTriggers] 	
