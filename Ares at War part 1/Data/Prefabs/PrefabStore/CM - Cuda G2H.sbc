    if (_artificialHorizon != null)
    {
        _artificialHorizon.UpdateFields(
                _sunRotationAxis,
                _skyColor,
                _groundColor,
                _progradeColor,
                _retrogradeColor,
                _textColor,
                _textBoxColor,
                _textBoxBackground,
