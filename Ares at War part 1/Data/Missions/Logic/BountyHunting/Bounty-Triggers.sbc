<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>

<!--
AaW_Bounty_Trigger_PlayerNear
AaW_Bounty_Trigger_Despawn
AaW_Bounty_Trigger_DespawnMES
AaW_Bounty_Trigger_Compromised


-->

 <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Mission_BountyTriggers</SubtypeId>
      </Id>
      <Description>
      [RivalAI TriggerGroup]

	[Triggers:AaW_Bounty_Trigger_PlayerNear]
	[Triggers:AaW_Bounty_Trigger_Despawn]
	[Triggers:AaW_Bounty_Trigger_DespawnMES]
	[Triggers:AaW_Bounty_Trigger_Compromised]

      </Description>
 </EntityComponent>
 
 


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Action_FailMission</SubtypeId>
      </Id>
      <Description>
      [RivalAI Action]

	[ActivateEvent:true]
	[ActivateEventIds:MES_EventTemplate_BountyHunting_Fail@{EventInstance}]

      </Description>
    </EntityComponent>
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Action_SuccessMission</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]

	[ResetCooldownTimeOfEvents:true]
	[ResetEventCooldownIds:MES_EventTemplate_BountyHunting_Success@{EventInstance}]

	[ToggleEvents:true]
	[ToggleEventIds:MES_EventTemplate_BountyHunting_Success@{EventInstance}]
	[ToggleEventIdModes:true]
      </Description>
      
    </EntityComponent>
	
	

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Trigger_PlayerNear</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
      [Type:PlayerNear]
	  [UseTrigger:true]
	  [StartsReady:true]
	  [TargetDistance:1200]
	  [MaxActions:1]
	  [Actions:AaW_Bounty_Action_PlayerNear]
	  
      </Description>
      
    </EntityComponent>
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Action_PlayerNear</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
      [UseChatBroadcast:true]
      [ChatData:AaW_Bounty_Chat_PlayerNear]

      </Description>
      
    </EntityComponent>

	
		<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Chat_PlayerNear</SubtypeId>
      </Id>
	  
      <Description>

      [RivalAI Chat]
      
      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:true]
      
      [Author:{AntennaName}]
      [Color:Red]
      
      [ChatMessages:{PlayerName}! How did you find me? Agh it doesn't matter. Prepare to die!]
      [ChatAudio:ArcHudGPSNotification1]
      [BroadcastChatType:Chat]
      
      </Description>
    </EntityComponent>
	
	
	
	

	
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Trigger_Despawn</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
      [Type:Despawn]
	  [UseTrigger:true]
	  [StartsReady:true]
	  [MaxActions:1]
	  [Actions:AaW_Bounty_Action_FailMission]
	  
      </Description>
      
    </EntityComponent>	



	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Trigger_DespawnMES</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
      [Type:DespawnMES]
	  [UseTrigger:true]
	  [StartsReady:true]
	  [MaxActions:1]
	  [Actions:AaW_Bounty_Action_FailMission]
	  
      </Description>
      
    </EntityComponent>	




	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Trigger_Compromised</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
      [Type:Compromised]
	  [UseTrigger:true]
	  [StartsReady:true]
	  [MaxActions:1]
	  [Actions:AaW_Bounty_Action_Compromised]
	  [Actions:AaW_Bounty_Action_SuccessMission]	  
      </Description>
      
    </EntityComponent>		
		
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Action_Compromised</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
      [UseChatBroadcast:true]
      [ChatData:AaW_Bounty_Chat_Compromised]

[GenerateExplosion:true]
[ExplosionRange:14]
[ExplosionDamage:5]
[ExplosionIgnoresVoxels:true]
      </Description>
      
    </EntityComponent>

	
		<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Bounty_Chat_Compromised</SubtypeId>
      </Id>
	  
      <Description>

      [RivalAI Chat]
      
      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:true]
      
      [Author:Pirate]
      [Color:Red]
      
      [ChatMessages:NOOOOOOO!!!]
      [ChatAudio:ArcHudGPSNotification1]
      [BroadcastChatType:Chat]
      
	  
	  [IgnoreAntennaRequirement:true]
	  [IgnoredAntennaRangeOverride:10000]
      </Description>
    </EntityComponent>
	

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
  </EntityComponents>
</Definitions>