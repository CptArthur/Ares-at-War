<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>

    <!--


        [Triggers:AaW_Cargoship_Trigger_TargetIsApproaching]
        [Triggers:AaW_Cargoship_Trigger_Surrender]




    -->


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Cargoship_Trigger_TargetIsApproaching</SubtypeId>
      </Id>
      <Description>

        [RivalAI Trigger]
        [Type:Manuel]

        [Tags:MSB_TriggerEvent_InitialTargetIsApproaching_Easy]
        [Tags:MSB_TriggerEvent_InitialTargetIsApproaching_Hard]
        [Tags:MSB_TriggerEvent_InitialTargetIsApproaching_Other]

        [//Conditions:AaW_Cargoship_Condition_TargetIsApproaching]

        [UseTrigger:true]
        [StartsReady:true]
        [MinCooldownMs:120000]
        [MaxCooldownMs:180000]
        [ActionExecution:Random]
        [Actions:AaW_Cargoship_Action_TargetIsApproaching]

      </Description>
    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>AaW_Cargoship_Condition_TargetIsApproaching</SubtypeId>
      </Id>
      <Description>
        [RivalAI Condition]

        [UseConditions:true]
        [MatchAnyCondition:false]

        [CheckIfTargetIsPlayerOwned:false]
        [CheckIfTargetIsNpcOwned:false]

      </Description>
    </EntityComponent>


	  <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Cargoship_Action_TargetIsApproaching</SubtypeId>
      </Id>
      <Description>

        [RivalAI Action]

        [PlayDialogueCue:true]
        [PlayDialogueCue:AaW_CargoShip_TargetIsApproaching]


        [DisableTriggers:true]
        [DisableTriggerNames:AaW_Cargoship_Trigger_TargetIsApproaching]

      </Description>
    </EntityComponent>



		<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Cargoship_Trigger_Surrender</SubtypeId>
      </Id>
      <Description>

        [RivalAI Trigger]
        [UseTrigger:true]
        [Type:Manual]
        [Tags:MSB_TriggerEvent_VeryLowMorale]
        [StartsReady:true]

        [Conditions:AaW_Cargoship_Condition_Surrender]



        [Actions:AaW_Cargoship_Action_Surrender]
      </Description>

    </EntityComponent>



	    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>AaW_Cargoship_Condition_Surrender</SubtypeId>
      </Id>
      <Description>
        [RivalAI Condition]

        [UseConditions:true]
        [MatchAnyCondition:false]

        [HasAntennaForBroadcast:true]


      </Description>
    </EntityComponent>




	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>AaW_Cargoship_Action_Surrender</SubtypeId>
      </Id>
      <Description>

        [RivalAI Action]


        [ManuallyActivateTrigger:true]
        [ManuallyActivatedTriggerTags:AaW_TriggerEvent_Terminate]
        [ForceManualTriggerActivation:false]


        [DisableAutopilot:true]


        [ToggleBlocksOfType:true]


        [BlockTypesToToggle:MyObjectBuilder_LargeGatlingTurret/(null)]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_LargeGatlingTurret/SmallGatlingTurret]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_LargeMissileTurret/(null)]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_LargeMissileTurret/SmallMissileTurret]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_InteriorTurret/LargeInteriorTurret]
        [BlockTypeToggles:Off]


        [BlockTypesToToggle:MyObjectBuilder_LargeMissileTurret/LargeCalibreTurret]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_LargeMissileTurret/LargeBlockMediumCalibreTurret]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_LargeMissileTurret/SmallBlockMediumCalibreTurret]
        [BlockTypeToggles:Off]

        [BlockTypesToToggle:MyObjectBuilder_LargeGatlingTurret/AutoCannonTurret]
        [BlockTypeToggles:Off]


        [PlayDialogueCue:true]
        [PlayDialogueCue:AaW_CargoShip_Surrender]




      </Description>

    </EntityComponent>


















  </EntityComponents>
</Definitions>