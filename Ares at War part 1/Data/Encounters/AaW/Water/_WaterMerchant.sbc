      <Description>
        [RivalAI Trigger]

        [Type:Timer]

        [UseTrigger:true]
        [StartsReady:true]

        [MaxActions:1]
        [Actions:AaW_WaterMerchant_Action_Setup]

      </Description>
    </EntityComponent>
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>AaW_WaterMerchant_Action_Setup</SubtypeId>
      </Id>
      <Description>
        [RivalAI Action]

        [SetBooleansTrue:AaW_Role_WaterMerchant]

                [ManuallyActivateTrigger:true]
        [ManuallyActivatedTriggerTags:MSB_Role_InitRole]
        
      </Description>
    </EntityComponent>

