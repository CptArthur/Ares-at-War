<!-- PlayerAgarisOuterOrbit-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GC_BlockadeLeader_Trigger_PlayerAgarisOuterOrbit</SubtypeId>
      </Id>
      <Description>

	[RivalAI Trigger]
	[Type:PlayerNear]
	[UsePlayerCondition:true]
	[PlayerConditionIds:PlayerCondition-Position-Agaris-OuterOrbit]
	[PlayerConditionIds:PlayerCondition-GCNeutral]
	[TargetDistance:9000]
	[UseTrigger:true]
	[StartsReady:true]
	
	[MinCooldownMs:450000]
	[MaxCooldownMs:600000]
	[Conditions:FAC_Context_Condition_SettingNormal]

	  [Actions:GC_BlockadeLeader_Action_PlayerAgarisOuterOrbit]	  
      </Description>
      
    </EntityComponent>	

