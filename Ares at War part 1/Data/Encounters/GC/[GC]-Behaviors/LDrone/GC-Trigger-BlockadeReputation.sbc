      </Id>
      <Description>

	[RivalAI Trigger]
	[Type:PlayerNear]
	[UsePlayerCondition:true]
	[PlayerConditionIds:PlayerCondition-Position-Agaris-OuterOrbit]
	[PlayerConditionIds:PlayerCondition-GCNeutral]
	[TargetDistance:9000]
	[UseTrigger:true]
	[StartsReady:true]
	
	[MinCooldownMs:5000]
	[MaxCooldownMs:5001]
	[Conditions:FAC_Context_Condition_SettingNormal]
	[Actions:GC_BlockadeLeader_Action_PlayerAgarisOuterOrbit_ReputationLoss]	  
      </Description>
      
    </EntityComponent>
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GC_BlockadeLeader_Action_PlayerAgarisOuterOrbit</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
      
      [UseChatBroadcast:true]
      [ChatData:GC_BlockadeLeader_Chat_PlayerAgarisOuterOrbit]
	  
      </Description>
      
    </EntityComponent>


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GC_BlockadeLeader_Action_PlayerAgarisOuterOrbit_ReputationLoss</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]
      
	[ChangeReputationWithPlayers:true]
	[ReputationChangeRadius:9000]
	[ReputationPlayerConditionIds:PlayerCondition-Position-Agaris-OuterOrbit]
	[ReputationPlayerConditionIds:PlayerCondition-GCNeutral]
	[ReputationChangeFactions:GC]
	[ReputationChangeAmount:-10]
	[ReputationChangesForAllRadiusPlayerFactionMembers:true]
	
      </Description>
      
    </EntityComponent>	


	
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GC_BlockadeLeader_Chat_PlayerAgarisOuterOrbit</SubtypeId>
      </Id>
	  
      <Description>

      [RivalAI Chat]
      
      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:-1]
      [BroadcastRandomly:true]
      
      [Author:GC Command]
      [Color:Red]
      
      [ChatMessages:Martial Law is now in effect for Agaris. No one is allowed to enter the planet. Leave immediately! ]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Chat]
	  
	[IgnoreAntennaRequirement:true]
	[IgnoredAntennaRangeOverride:20000]
	  
	  
      </Description>
      
    </EntityComponent>

