<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<SpawnGroups>

	<!-- Sunset City Base -->
	<SpawnGroup>
		<Id>
			<TypeId>SpawnGroupDefinition</TypeId> 
			<SubtypeId>GCSunsetCity</SubtypeId>
		</Id>
		<Description>
		[Modular Encounters SpawnGroup]
		[AdminSpawnOnly:true]

		[ForceStaticGrid:true]

		[FactionOwner:GC]		

		[PlanetaryInstallation:true]
		[PlanetaryInstallationType:Medium]


		[UseRandomNameGenerator:true]
		
		[RandomGridNamePattern:SunsetCity Base]
		[ReplaceAntennaNameWithRandomizedName:Antenna]
[PlanetWhitelist:Planet Agaris]
		[ReplenishSystems:true]
		[UseRivalAi:true]
		[RivalAiReplaceRemoteControl:true]
		[IgnoreCombatPhase:true]
		[IgnoreCleanupRules:true]
		[SandboxVariables:GCSunsetCity]			
		[ZoneConditions:ZoneCondition_ZoneAaW]
		</Description>
		<Icon>Textures\GUI\Icons\Fake.dds</Icon>  
		<IsPirate>true</IsPirate>
		
		<Frequency>5.0</Frequency>
		<Prefabs>
			<Prefab SubtypeId="GC-HeavyBase">
				<Position>
					<X>0.0</X>
					<Y>-88</Y>
					<Z>0.0</Z>
				</Position>
				<Speed>0</Speed>
				<Behaviour>GC_CapturableController_Behavior</Behaviour>
			<PlaceToGridOrigin>true</PlaceToGridOrigin>
			</Prefab>
		</Prefabs>
		
	</SpawnGroup>
	



