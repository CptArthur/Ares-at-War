<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SpawnGroups>


    <SpawnGroup>
      <Id>
        <TypeId>SpawnGroupDefinition</TypeId>
        <SubtypeId>GC-SpawnGroup-Destroyer</SubtypeId>
      </Id>
      <Description>

        [Modular Encounters SpawnGroup]


		[SpawnConditionsProfiles:GC-SpawnProfile-AgarisNaval-Both-Zone]
		[SpawnConditionsProfiles:GC-SpawnProfile-AgarisNaval-Both-Outside]
		[SpawnConditionsProfiles:GC-SpawnProfile-AgarisNaval-Combat]


        [UseRivalAi:true]
        [RivalAiReplaceRemoteControl:true]


		[UseRandomNameGenerator:true]
		[RandomGridNamePrefix:(NPC-AAW)]
		[RandomGridNamePattern:Destroyer-RandomLetterRandomNumberRandomNumber]
		[ReplaceAntennaNameWithRandomizedName:Destroyer]	

      </Description>
      <IsPirate>true</IsPirate>
      <Frequency>10.0</Frequency>
      <Prefabs>
        <Prefab SubtypeId="GC-MainBoat">
          <Position>
            <X>0.0</X>
            <Y>0.0</Y>
            <Z>0.0</Z>
          </Position>
          <Speed>25</Speed>
          <Behaviour>GC-Behaviour-Destroyer</Behaviour>
        </Prefab>
      </Prefabs>
    </SpawnGroup>


    <SpawnGroup>
      <Id>
        <TypeId>SpawnGroupDefinition</TypeId>
        <SubtypeId>GC-SpawnGroup-PatrolBoat</SubtypeId>
      </Id>
      <Description>

        [Modular Encounters SpawnGroup]

		[SpawnConditionsProfiles:GC-SpawnProfile-AgarisNaval-Both-Zone]
		[SpawnConditionsProfiles:GC-SpawnProfile-AgarisNaval-Both-Outside]
		[SpawnConditionsProfiles:GC-SpawnProfile-AgarisNaval-Combat]

        [UseRivalAi:true]
        [RivalAiReplaceRemoteControl:true]

		[UseRandomNameGenerator:true]
		[RandomGridNamePrefix:(NPC-AAW)]
		[RandomGridNamePattern:The ColorAdjective FishNoun]
		[ReplaceAntennaNameWithRandomizedName:Patrol boat]
      </Description>
      <IsPirate>true</IsPirate>
      <Frequency>10.0</Frequency>
      <Prefabs>
        <Prefab SubtypeId="GC-PatrolBoat">
          <Position>
            <X>0.0</X>
            <Y>0.0</Y>
            <Z>0.0</Z>
          </Position>
          <Speed>25</Speed>
          <Behaviour>GC-Behaviour-RaidingBoat</Behaviour>
        </Prefab>
      </Prefabs>
    </SpawnGroup>
	
	
	
	
	
	
	
	
	
	
	
	

  </SpawnGroups>
</Definitions>