		[StaticEncounterForward:{X:-0.0982735579486698 Y:0.987168227796248 Z:0.125861820413198}]
		[StaticEncounterUp:{X:-0.0886990191253776 Y:0.117281823399077 Z:-0.989129646662349}]

		[StaticEncounterUsePlanetDirectionAndAltitude:true]
		[StaticEncounterPlanet:Planet Agaris]
		[StaticEncounterPlanetDirection:{X:-0.0893169225760334 Y:0.117425480867419 Z:-0.989056997237572}]
		[StaticEncounterPlanetAltitude:0.9537128468437]


		[ReplenishSystems:true]
		[UseRivalAi:true]
		[RivalAiReplaceRemoteControl:true]
		[IgnoreCombatPhase:true]
		[IgnoreCleanupRules:true]
		[SandboxVariables:GCSunsetCity]			
		[ZoneConditions:ZoneCondition-ZoneAaW]

		</Description>
		<Icon>Textures\GUI\Icons\Fake.dds</Icon>  
		<IsPirate>true</IsPirate>
		
		<Frequency>5.0</Frequency>
		<Prefabs>
			<Prefab SubtypeId="GC-HeavyBase">
				<Position>
					<X>0.0</X>
					<Y>0.0</Y>
					<Z>0.0</Z>
				</Position>
				<Speed>0</Speed>
				<Behaviour>GC-ColonyController-Behavior</Behaviour>
			<PlaceToGridOrigin>true</PlaceToGridOrigin>
			</Prefab>
		</Prefabs>
		
	</SpawnGroup>
	



