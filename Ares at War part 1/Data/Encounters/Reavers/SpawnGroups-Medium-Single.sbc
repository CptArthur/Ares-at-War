    <!--Cockroach-->
    <SpawnGroup>
      <Id>
        <TypeId>SpawnGroupDefinition</TypeId>
        <SubtypeId>AaW-Reaver-SpawnGroup-Cockroach</SubtypeId>
      </Id>
      <Description>

        [Modular Encounters SpawnGroup]

        [ChanceCeiling:250]
		[CustomApiConditions:BylenRing]
        [SpaceCargoShip:true]
        [SpaceCargoShipChance:75]

        [AtmosphericCargoShip:true]
        [AtmosphericCargoShipChance:1]
        [SkipAirDensityCheck:true]

        [GravityCargoShip:true]
        [GravityCargoShipChance:10]

        [AddDefenseShieldBlocks:true]

        [FactionOwner:REAVER]
        [ReplenishSystems:true]

        [ManipulationProfiles:Reaver-Manipulation-Common]
        [ManipulationProfiles:Reaver-Manipulation-Loot]
        [ManipulationProfiles:Reaver-Manipulation-Thrust]
        [ManipulationProfiles:Reaver-Manipulation-PaintAndBuildStates]

        [MaxGravity:1]

        [UseThreatLevelCheck:true]
        [ThreatLevelCheckRange:10000]
        [ThreatScoreMinimum:666]

        [RequireAnyMods:2146772509]
        [RequireAnyMods:1945915721]
        [RequireAnyMods:1902970975]
        [RequireAnyMods:1464803311]

      </Description>
      <IsPirate>true</IsPirate>
      <Frequency>1.0</Frequency>
      <Prefabs>
        <Prefab SubtypeId="(NPC-RVR) Cockroach">
          <Position>
            <X>0.0</X>
            <Y>0.0</Y>
            <Z>0.0</Z>
          </Position>
          <Speed>25</Speed>
          <Behaviour>Reaver-Behavior-Cockroach</Behaviour>
        </Prefab>
