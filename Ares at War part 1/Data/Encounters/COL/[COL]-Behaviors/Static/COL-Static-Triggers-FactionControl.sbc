	[Conditions:COL-Condition-GC]
	 
[ActionExecution:All]
	 
	[Actions:COL-Static-Action-GC]
	[Actions:COL-Static-Action-GC2]
      </Description>
      
    </EntityComponent>
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>COL-Static-Action-GC</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
      
	[ChangeBlockOwnership:true]
	[OwnershipBlockNames:{FAC}]
	[OwnershipBlockFactions:GC]

	[RecolorGrid:true]
	[OldBlockColors:{X:0 Y:-0.8 Z:-0.13}]
	[NewBlockColors:{X:0 Y:-0.03 Z:-0.14}]

	[EnableTriggers:true]
	[DisableTriggers:true]
	
	[DisableTriggerNames:FAC-Trigger-CallAtmoCargoShip-UCACode]
	[EnableTriggerNames:FAC-Trigger-CallAtmoCargoShip-GCCode]
	
      </Description>
      
    </EntityComponent>	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
