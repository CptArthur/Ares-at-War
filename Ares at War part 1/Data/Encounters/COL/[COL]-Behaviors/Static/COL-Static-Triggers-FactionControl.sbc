	[OldBlockColors:{X:0.3333333 Y:-0.48 Z:-0.25}]
	[NewBlockColors:{X:0 Y:-0.03 Z:-0.14}]


      </Description>
      
    </EntityComponent>		
	
	
	
	
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>COL-Static-Triggers-COL</SubtypeId>
      </Id>
      <Description>

	[RivalAI Trigger]
	[UseTrigger:true]
    [Type:Session]
	[Conditions:COL-Condition-COL]
	  
	  
	[ActionExecution:All]  
	[Actions:COL-Static-Action-COL]
	[Actions:COL-Static-Action-COL2]
