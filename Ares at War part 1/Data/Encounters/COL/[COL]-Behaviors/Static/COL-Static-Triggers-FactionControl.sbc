<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>
  
<!-- 
[Triggers:COL-Static-Triggers-FAF]
[Triggers:COL-Static-Triggers-GC]
[Triggers:COL-Static-Triggers-COL]


-->

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>COL-Static-Triggers-FAF</SubtypeId>
      </Id>
      <Description>

	[RivalAI Trigger]
	[UseTrigger:true]
    [Type:Session]
	[Conditions:COL-Condition-FAF]

[ActionExecution:All]
	[Actions:COL-Static-Action-FAF]
	[Actions:COL-Static-Action-FAF2]
      </Description>
      
    </EntityComponent>

<!-- Sunset City-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>COL-Static-Action-FAF</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]

	
	[RecolorGrid:true]
	[OldBlockColors:{X:0 Y:-0.8 Z:-0.13}]
	[NewBlockColors:{X:0.3333333 Y:-0.48 Z:-0.25}]

	[ChangeBlockOwnership:true]
	[OwnershipBlockNames:{FAC}]
	[OwnershipBlockFactions:FAF]

	[EnableTriggers:true]
	[DisableTriggers:true]
	
			
	[EnableTriggerNames:FAC-Trigger-CallAtmoCargoShip-FAFCode]
	[DisableTriggerNames:FAC-Trigger-CallAtmoCargoShip-GCCode]

      </Description>
      
    </EntityComponent>	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>COL-Static-Action-FAF2</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]

	[RecolorGrid:true]
	[OldBlockColors:{X:0 Y:-0.03 Z:-0.14}]
	[NewBlockColors:{X:0.3333333 Y:-0.48 Z:-0.25}]



      </Description>
      
    </EntityComponent>	
	

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
