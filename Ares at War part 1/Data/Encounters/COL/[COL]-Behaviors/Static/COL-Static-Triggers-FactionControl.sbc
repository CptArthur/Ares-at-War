      </Id>
      <Description>

      [RivalAI Action]
      
	[RecolorGrid:true]

	[NewBlockColors:{X:0.3333333 Y:-0.48 Z:-0.25}]
	[NewBlockColors:{X:0 Y:-0.8 Z:-0.13}]


      </Description>
      
    </EntityComponent>	





