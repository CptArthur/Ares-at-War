<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>

  <!-- [Triggers:ITC_Trigger_Static_PopulateStores_ITCCapital] -->
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>ITC_Trigger_Static_PopulateStores_ITCCapital</SubtypeId>
      </Id>
      <Description>

        [RivalAI Trigger]

        [UseTrigger:true]
        [Type:Timer]
        [MinCooldownMs:1200000]
        [MaxCooldownMs:1200001]
        [Conditions:ITC_Condition_Static_PopulateStores_ITCCapital]
        [StartsReady:true]
        [MaxActions:-1]
        [Actions:ITC_Action_Static_PopulateStores_ITCCapital]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>ITC_Condition_Static_PopulateStores_ITCCapital</SubtypeId>
      </Id>
      <Description>

        [RivalAI Condition]
              
        [UseConditions:true]
        [UseFailCondition:true]
        [CheckAllLoadedModIDs:true]
        [AllModIDsToCheck:2344068716]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>ITC_Action_Static_PopulateStores_ITCCapital</SubtypeId>
      </Id>
      <Description>

        [RivalAI Action]

        [ApplyStoreProfiles:true]
        [ClearStoreContentsFirst:true]

       [StoreBlocks:Store Settlement]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Settlement]		



      [StoreBlocks:Store {ITC}]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_ITC]		
		
	    [StoreBlocks:Store {SYN}]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_SYN]	


      [StoreBlocks:Store Civ]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Civ]
  
      [StoreBlocks:Store Salvage]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Salvage]

      [StoreBlocks:Store Ammo]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Ammo]

      [StoreBlocks:Store Ingot]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Ingot]
  
      [StoreBlocks:Store Tool]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Tool]      

      [StoreBlocks:Store Prefab]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Prefab]      

      
      [StoreBlocks:Store Fuel]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Fuel]
  
      
      [StoreBlocks:Store Outpost]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_Outpost]


      [StoreBlocks:Vending Machine]
      [StoreProfiles:ITC_StoreProfile_ITCCapital_VendingMachine]




      [AddCustomDataToBlocks:true]
      [CustomDataBlockNames:EconomySurvival Store Water Vehicles,EconomySurvival Store Land Vehicles,EconomySurvival Store Air Vehicles,EconomySurvival Store Space Vehicles]
      [CustomDataFiles:ITC_Water_Vehicles.xml]
      [CustomDataFiles:ITC_Land_Vehicles.xml]
      [CustomDataFiles:ITC_Air_Vehicles.xml]
      [CustomDataFiles:ITC_Space_Vehicles.xml]  
        
      [ChangeBlockOwnership:true]
      [OwnershipBlockNames:Contracts {SYN}]
      [OwnershipBlockFactions:SHIVAN]

      [OwnershipBlockNames:Contracts {ITC}]
      [OwnershipBlockFactions:ITC]
    

      [ApplyContractProfiles:true]
      [ClearContractContentsFirst:true]
      [ContractBlocks:Contracts]
      [ContractBlockProfiles:ITC_ContractBlockProfile_ITCCapital]

       [ContractBlocks:Contracts {SYN}]
      [ContractBlockProfiles:SYN_ContractBlockProfile_Civilian]

       [ContractBlocks:Contracts {ITC}]
      [ContractBlockProfiles:ITC_ContractBlockProfile_Civilian]
      


      </Description>

    </EntityComponent>
  </EntityComponents>
</Definitions>
