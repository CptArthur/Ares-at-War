<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <PlanetGeneratorDefinitions>
      <PlanetGeneratorDefinition>
    <!--<Definition xsi:type="PlanetGeneratorDefinition">-->
        <Id>
            <TypeId>PlanetGeneratorDefinition</TypeId>
            <SubtypeId>Blackhole (BalCorp)</SubtypeId>
        </Id>

        <MusicCategories>
            <MusicCategory Category="Danger" Frequency="0.075" />
        </MusicCategories>

        <SurfaceGravity>10000</SurfaceGravity>
        <GravityFalloffPower>5.6</GravityFalloffPower>
        <DefaultSurfaceMaterial Material="BalCorp_Blackhole" MaxDepth="1"/>
        <DefaultSubSurfaceMaterial Material="BalCorp_Blackhole"/>
        
        <PlanetMaps Material="false" Ores="false" Biome="false" Occlusion="false"/>
        
        <MaterialsMaxDepth Min="4000" Max="4000"/>
        <MaterialsMinDepth Min="0" Max="20"/>

        <HillParams Min = "-0.0003" Max ="-0.0002"/>

        <HasAtmosphere>true</HasAtmosphere>
        <Atmosphere>
            <Breathable>false</Breathable>
            <Density>0.001</Density>
            <LimitAltitude>10000.0</LimitAltitude>
        </Atmosphere>

        <AtmosphereSettings>
            <RayleighScattering>
                <X>15.786</X>
                <Y>37.753</Y>
                <Z>100.000</Z>
            </RayleighScattering>
            <MieScattering>147.671082</MieScattering>
            <MieColorScattering>
                <X>5.000</X>
                <Y>5.000</Y>
                <Z>5.000</Z>
            </MieColorScattering>
            <RayleighHeight>1.000</RayleighHeight>
            <RayleighHeightSpace>1.000</RayleighHeightSpace>
            <RayleighTransitionModifier>0.100</RayleighTransitionModifier>
            <MieHeight>25.000</MieHeight>
            <MieG>1</MieG>
            <Intensity>0.100</Intensity>
            <FogIntensity>0</FogIntensity>
            <SeaLevelModifier>0.9</SeaLevelModifier>
            <AtmosphereTopModifier>1.1</AtmosphereTopModifier>
            <Scale>1</Scale>
            <SunColor>
                <X>1</X>
                <Y>1</Y>
                <Z>1</Z>
            </SunColor>
            <SunSpecularColor>
                <X>1</X>
                <Y>1</Y>
                <Z>1</Z>
            </SunSpecularColor>
        </AtmosphereSettings>
        
        <CloudLayers>
            <CloudLayer>
                <Model>Models\Environment\BalCorp_Blackhole\BalCorp_BlackHoleDarkRing.mwm</Model>
                <Textures>
                    <Texture>Textures\Models\Environment\BalCorp_Blackhole\Blackhole_Misc.dds</Texture>
                </Textures>
                <RelativeAltitude>1.0</RelativeAltitude>
                <RotationAxis>
                    <X>0</X>
                    <Y>1</Y>
                    <Z>0</Z>
                </RotationAxis>
                <AngularVelocity>0.0</AngularVelocity>
                <InitialRotation>0.0</InitialRotation>
                <ScalingEnabled>false</ScalingEnabled>
                <FadeOutRelativeAltitudeStart>0.0</FadeOutRelativeAltitudeStart>
                <FadeOutRelativeAltitudeEnd>0.0</FadeOutRelativeAltitudeEnd>
                <ApplyFogRelativeDistance>0.001</ApplyFogRelativeDistance>
            </CloudLayer>
            <CloudLayer>
