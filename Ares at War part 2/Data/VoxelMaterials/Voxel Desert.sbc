      <Far2Scale>1200</Far2Scale>

      <Far3Distance>25000</Far3Distance>
      <Far3Color>
        <X>0.07</X>
        <Y>0.07</Y>
        <Z>0</Z>
        <W>1.0</W>
      </Far3Color>
    


      <DamagedMaterial>DarkRock</DamagedMaterial>

      <ColorKey Hex="#BCBB56"/>
    </VoxelMaterial>	
	
	
	
