	  <FolderName>Planet Crait</FolderName>
	  
	  
      <PlanetMaps Material="false" Ores="true" Biome="true" Occlusion="true"/>
  

  
  
  <MesherPostprocessing xsi:type="MyObjectBuilder_VoxelMesherComponentDefinition">
