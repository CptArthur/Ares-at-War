      <Components>
        <Component Subtype="SteelPlate" Count="120" />
        <Component Subtype="MetalGrid" Count="150" />
        <Component Subtype="GravityGenerator" Count="90" />
        <Component Subtype="Detector" Count="60" />
        <Component Subtype="JumpCore" Count="15" />		
        <Component Subtype="PowerCell" Count="360" />
        <Component Subtype="Superconductor" Count="4200" />
        <Component Subtype="Computer" Count="900" />
        <Component Subtype="Construction" Count="540" />
        <Component Subtype="SteelPlate" Count="60" />
      </Components>
      <CriticalComponent Subtype="Computer" Index="0" />


      <ResourceSinkGroup>Charging</ResourceSinkGroup>
      <RequiredPowerInput>64</RequiredPowerInput>
      <PowerNeededForJump>5</PowerNeededForJump>
      <PowerEfficiency>0.9</PowerEfficiency>

      <MaxJumpDistance>6000000</MaxJumpDistance>
      <MaxJumpMass>2500000</MaxJumpMass>

    </Definition>

