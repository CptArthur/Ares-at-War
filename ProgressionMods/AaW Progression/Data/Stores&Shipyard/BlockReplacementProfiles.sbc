  <EntityComponents>
<!-- Do not change the <SubtypeIds> otherwise AaW can't reference them. 


If you want to adjust at encounter level contact me.  But for now MinorSettlement, MajorSettlement and Faction specfic should suffice, I think - CptArthur





-->	

<!-- Minor settlments:
Examples: Doohan, Thorrix, Bratis, Azuris
-->

     <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>AaW_ProgressionBlockReplacement_MinorSettlement</SubtypeId>
      </Id>
      <Description>
        [MES Block Replacement]
	
		[OldBlock:MyObjectBuilder_CubeBlock/Placeholder_AQD_SG_Jumpdrive]
		[NewBlock:JumpDrive/AQD_SG_Jumpdrive]				
		
	
      </Description>
    </EntityComponent>


<!-- Major settlements:
Examples: Station27, Starlight City, Sunset City, Rak Station
-->
     <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>AaW_ProgressionBlockReplacement_MajorSettlement</SubtypeId>
      </Id>
      <Description>
        [MES Block Replacement]
	
		[OldBlock:MyObjectBuilder_CubeBlock/Placeholder_LargeJumpDrive]
		[NewBlock:JumpDrive/LargeJumpDrive]			
		[OldBlock:MyObjectBuilder_CubeBlock/Placeholder_AQD_SG_Jumpdrive]
		[NewBlock:JumpDrive/AQD_SG_Jumpdrive]				
		
		[OldBlock:MyObjectBuilder_CubeBlock/Placeholder_SmallBlockSmallThrust]
		[NewBlock:Thrust/SmallBlockSmallThrust]		
